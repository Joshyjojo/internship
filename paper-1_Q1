#include<iostream>
using namespace std;
int main()
{
    int arr[] = {12, 13, 1, 10, 34, 1};
    int small = arr[0];
    for(int i=0;i<6;i++)
    {
        if(arr[i]<small)
          small=arr[i];
    }
    int ssmall=arr[0];
    for(int i=0;i<6;i++)
    {
        if(arr[i]<ssmall && arr[i]>small)
         ssmall=arr[i];
    }
    cout<<"The smallest number is\t"<<small;
    cout<<"\n The second smallest number is\t"<<ssmall;
    return 0;
}
